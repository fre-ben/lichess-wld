{
  "manifest_version": 3,
  "name": "Lichess WLD",
  "version": "1.0.0",
  "action": { "default_popup": "index.html" },
  "content_scripts": [
    {
      "js": ["src/scripts/helloWorld.tsx"],
      "matches": ["https://www.google.com/*"]
    },
    {
      "js": ["src/scripts/wld.tsx"],
      "matches": ["https://lichess.org/*"]
    }
  ]
}
